$query->bindValue(':id', $id, PDO::PARAM_INT);
$query->bindValue(':shipping_date', $shipping_date, PDO::PARAM_DATE);
$query->bindValue(':order_origin', $order_origin, PDO::PARAM_STR);
$query->bindValue(':order_creation_date', $order_creation_date, PDO::PARAM_DATE);
$query->bindValue(':reference', $reference, PDO::PARAM_STR);
$query->bindValue(':customer_type', $customer_type, PDO::PARAM_STR);
$query->bindValue(':language', $language, PDO::PARAM_STR);
$query->bindValue(':country', $country, PDO::PARAM_STR);
$query->bindValue(':export', $export, PDO::PARAM_BOOL);
$query->bindValue(':carrier', $carrier, PDO::PARAM_STR);
$query->bindValue(':carrier_date', $carrier_date, PDO::PARAM_DATE);
$query->bindValue(':recipient', $recipient, PDO::PARAM_STR);
$query->bindValue(':shipping_adress', $shipping_adress, PDO::PARAM_STR);
$query->bindValue(':phone', $phone, PDO::PARAM_STR);
$query->bindValue(':email', $email, PDO::PARAM_STR);
$query->bindValue(':item_reference', $item_reference, PDO::PARAM_STR);
$query->bindValue(':item_name_color', $item_name_color, PDO::PARAM_STR);
$query->bindValue(':width', $width, PDO::PARAM_STR);
$query->bindValue(':height', $height, PDO::PARAM_STR);
$query->bindValue(':roller_number_cm', $roller_number_cm, PDO::PARAM_STR);
$query->bindValue(':square_meter', $square_meter, PDO::PARAM_STR);
$query->bindValue(':billing_declik', $billing_declik, PDO::PARAM_STR);
$query->bindValue(':meter_square_ananbo', $meter_square_ananbo, PDO::PARAM_STR);
$query->bindValue(':impress_again', $impress_again, PDO::PARAM_BOOL);
$query->bindValue(':order_date_declik', $order_date_declik, PDO::PARAM_DATE);
$query->bindValue(':customer_notif', $customer_notif, PDO::PARAM_DATE);
$query->bindValue(':baseline', $baseline, PDO::PARAM_BOOL);
$query->bindValue(':print_file_date', $print_file_date, PDO::PARAM_DATE);
$query->bindValue(':origin_certificate_date', $origin_certificate_date, PDO::PARAM_DATE);
$query->bindValue(':shipping_email_date', $shipping_email_date, PDO::PARAM_DATE);
$query->bindValue(':infos', $infos, PDO::PARAM_STR);
$query->bindValue(':downloaded_file', $downloaded_file, PDO::PARAM_BOOL);
$query->bindValue(':declik_validate', $declik_validate, PDO::PARAM_BOOL);
$query->bindValue(':print_state_item', $print_state_item, PDO::PARAM_BOOL);
$query->bindValue(':shipping_number', $shipping_number, PDO::PARAM_STR);
